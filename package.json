{
  "name": "web-ext-deploy",
  "version": "0.14.0",
  "description": "A tool for deploying WebExtensions to multiple stores.",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "dist-esm/*.d.ts"
      ]
    }
  },
  "bin": {
    "web-ext-deploy": "dist-esm/index.js"
  },
  "types": "./dist-esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/avi12/web-ext-deploy"
  },
  "keywords": [
    "web-ext",
    "web-ext-deploy",
    "web-ext-publish",
    "chrome-deploy",
    "chrome-publish",
    "chrome-extension",
    "firefox-deploy",
    "firefox-publish",
    "firefox-extension",
    "firefox-addon",
    "firefox-submission-api",
    "opera-deploy",
    "opera-publish",
    "opera-extension",
    "opera-addon",
    "edge-deploy",
    "edge-publish",
    "edge-extension",
    "edge-publish-api"
  ],
  "files": [
    "dist-esm/**/*"
  ],
  "funding": [
    {
      "type": "PayPal",
      "url": "https://paypal.me/avi12"
    },
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/avi12"
    }
  ],
  "author": {
    "name": "Avi",
    "email": "avi6106@gmail.com",
    "url": "https://avi12.com"
  },
  "bugs": {
    "email": "avi6106@gmail.com",
    "url": "https://github.com/avi12/web-ext-deploy/issues"
  },
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/avi12/web-ext-deploy#readme",
  "scripts": {
    "debug:dev": "nodemon --config nodemon-dev.json",
    "debug:prod": "nodemon --",
    "clean": "rimraf dist-esm",
    "prepare": "pnpm clean && pnpm build",
    "build": "pnpm build:esm",
    "build:esm": "tsc",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "reinstall-locally": "pnpm unlink -g && pnpm i && pnpm link -g"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm lint:fix",
      "pnpm format",
      "git add"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/chrome-webstore-upload": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.16.0",
    "eslint-config-avi12": "github:avi12/eslint-config-avi12",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.13.0",
    "http-status": "^2.0.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite-node": "^2.1.8"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "chalk": "^5.3.0",
    "change-case": "^5.4.4",
    "chrome-webstore-upload": "^3.1.4",
    "dotenv": "^16.4.5",
    "exponential-backoff": "^3.1.1",
    "form-data": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "playwright": "^1.49.0",
    "yargs": "^17.7.2",
    "zip-local": "^0.3.5"
  },
  "packageManager": "pnpm@9.14.4"
}
